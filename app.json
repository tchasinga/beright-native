{
  "expo": {
    "name": "beright",
    "slug": "beright",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/play_store_512.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/play_store_512.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    }, 
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/play_store_512.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.tchasinga.berightnative"
    },
    "web": {
      "favicon": "./assets/play_store_512.png"
    },
    "extra": {
      "eas": {
        "projectId": "ccc8ab40-1feb-45de-8756-cfe8d85cf061"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "enabled": true,
      "checkAutomatically": "ON_LOAD"
    }
  }
}
